                  <Button fx:id="btnTransferir" mnemonicParsing="false" prefHeight="42.0" prefWidth="120.0" style="-fx-background-color: #00796B;" text="Transferir" textFill="WHITE" />
               </children>
            </HBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="lblBienvenido" text="Label" />
                        <Separator prefHeight="0.0" prefWidth="531.0" />
                        <Label fx:id="lblNumeroDeCuenta" text="Label" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Label text="Panel de administracion">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
